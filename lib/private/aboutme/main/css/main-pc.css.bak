	cursor: pointer;
	border-left:3px solid #6495ED;
}


.rightcontent {
    background-color:rgba(255,255,255,0.7);
	filter:alpha(opacity = 70);/*兼容ie*/
        border-radius:25px;
	opacity:0.7;
	height: 500px;
	min-height:500px;
	width: 60%;
	margin-right:6%; 
	float: right;
	overflow-y: scroll;
	box-shadow: 0px 6px 18px #CCC;
}

.rightcontent::-webkit-scrollbar {
    width: 0px;
}

.ad{
        border-radius:25px;
        margin: 0px 0px 15px 0px;
        width:300px;
        height:220px;
        border: #ddd;
        background: whitesmoke;
        right: 0px;
        bottom: 0px;
        position: fixed;
	text-align:center;
	-webkit-animation: ad .4s;
	animation: ad .4s;
		}
		
		@keyframes ad {
		    0% {
		        -webkit-transform: translate3d(0, 100%, 0);
		        -webkit-transform: translate3d(0, 100%, 0);
		        transform: translate3d(0, 100%, 0);
		        transform: translate3d(0, 100%, 0);
		    }
		    100% {
		        -webkit-transform: none;
		        transform: none;
		    }
		}
		
		@-webkit-keyframes ad {
		    0% {
		        -webkit-transform: translate3d(0, 100%, 0);
		    }
		    100% {
		        -webkit-transform: none;
		    }
		}

