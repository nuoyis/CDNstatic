@media screen and (min-width: 1281px) and ( max-width: 1980px) {
html,body {
    margin: 0;
    padding: 0;
    width: 100%;
    min-height: 100vh;
	font-family:Helvetica,Arial,sans-serif;
	background: url(https://api.vvhan.com/api/acgimg);
    background-repeat: no-repeat;
    background-attachment: fixed;
    background-size: cover;
    -webkit-background-size: cover;
    -o-background-size: cover;
    background-position: center 0;
/*	overflow: hidden; */
}
ul {
	list-style: none;
	padding: 0;
    text-align: left;
}

button{
	bottom: 10px;
        float: right;
	position: absolute;
	transform: translateX(-50%);
}


header {
	background: #ffe5e5;
	color: #EE0000;
	height: 60px;
}
header img {
	border-radius: 80px;
}
header .headimg {
	margin: 0px 10px 10px 30px;
	padding-top: 14px;
}
header .headimg img{
	width: 30px;
	height: 30px;
	vertical-align: middle;
}
header .headimg .name {
	margin-left: 15px;
	font-size: 25px;
	padding-bottom: 10px;
}
header .icon {
	margin: 10px 0px 5px 30px;
}
header .icon span {
	margin: 0px 8px;
	padding:4px;
}
header .icon img{
	width:25px;
	height: 25px;
	transition:1.5s;
}
header .icon img:hover {
	transform: scale(1.5) rotate(360deg);
}

.xw-loader {
		  width: 100%;
		  height: 100%;
		  position: absolute;
		  top: 0;
		  left: 0;
		  background-color: #66CCFF;
		  display:flex;
		  justify-content: center;
		  align-items: center;
		}
		.xw-dot {
		  width: 40px;
		  height: 40px;
		  background: #3ac;
		  border-radius: 100%;
		  display: inline-block;
		  animation: slide 1s infinite;
		}
		.xws {
		  margin: 0;
		  height: 0px;
		  width: 150px;
		  margin-top: 150px;
		  text-align: center;
		  }
		  
		.xw-dot:nth-child(1) {
		  animation-delay: 0.1s;
		  background: #32aacc;
		}
		.xw-dot:nth-child(2) {
		  animation-delay: 0.2s;
		  background: #64aacc;
		}
		.xw-dot:nth-child(3) {
		  animation-delay: 0.3s;
		  background: #96aacc;
		}
		.xw-dot:nth-child(4) {
		  animation-delay: 0.4s;
		  background: #c8aacc;
		}
		.xw-dot:nth-child(5) {
		  animation-delay: 0.5s;
		  background: #faaacc;
		}
		@-moz-keyframes slide {
		  0% {
		    transform: scale(1);
		  }
		  50% {
		    opacity: 0.3;
		    transform: scale(2);
		  }
		  100% {
		    transform: scale(1);
		  }
		}
		@-webkit-keyframes slide {
		  0% {
		    transform: scale(1);
		  }
		  50% {
		    opacity: 0.3;
		    transform: scale(2);
		  }
		  100% {
		    transform: scale(1);
		  }
		}
		@-o-keyframes slide {
		  0% {
		    transform: scale(1);
		  }
		  50% {
		    opacity: 0.3;
		    transform: scale(2);
		  }
		  100% {
		    transform: scale(1);
		  }
		}
		@keyframes slide {
		  0% {
		    transform: scale(1);
		  }
		  50% {
		    opacity: 0.3;
		    transform: scale(2);
		  }
		  100% {
		    transform: scale(1);
		  }
		}

.xw-Googleadsense {
    position:absolute;
	width: 20%;
	margin:16% 0 0 5%;
	padding: 6px 0px 6px 0px;
	transition: 1s 10s;
	float: left;
}

#xwzs3 {
    z-index: 301;
    position: absolute;
    right: 0;
    top: 0;
    margin: 17px 9px 5px 0;
    padding: 0;
    padding-right: 0px;
    transition:5s;
}

#xwzs2{  
	margin: 0px 0px 10px 10px;
	border: 3px solid #00a0ff;
                border-radius: 50%;  
                transition: all 2.0s;  
}  

#xwzs2:hover{  
                 transform: rotate(360deg);
	 box-shadow: 0 0 10px #fff;
	 -webkit-box-shadow: 0 0 19px #fff;
	 transform:rotate(360deg);
	 -ms-transform:rotate(360deg); 	/* IE 9 */
	 -moz-transform:rotate(360deg); 	/* Firefox */
	 -webkit-transform:rotate(360deg); /* Safari 和 Chrome */
	 -o-transform:rotate(360deg); 	/* Opera */
	 filter:progid:DXImageTransform.Microsoft.BasicImage(rotation=3);
}  

.links {
    margin-bottom: 80px
}

.links ul {
    display: block;
    list-style-type: disc;
    margin-block-start: 1em;
    margin-block-end: 1em;
    margin-inline-start: 0px;
    margin-inline-end: 0px;
    padding-inline-start: 40px;
}

.links ul {
    margin: 0;
    margin-top: 50px;
    list-style: none;
    padding: 0;
    width: 100%;
    display: inline-block
}

.links ul li {
    width: 32%;
    float: left;
    border: 1px solid #505050;
    padding: 15px 15px;
    margin: 4px 4px;
    position: relative;
    overflow: hidden;
    -webkit-transition: all .3s;
    transition: all .3s;
    border-radius: 10px
}

.links ul li img {
    float: left;
    box-shadow: inset 0 0 10px #505050;
    padding: 5px;
    opacity: 1;
    transform: rotate(0);
    -webkit-transform: rotate(0);
    -moz-transform: rotate(0);
    -o-transform: rotate(0);
    -ms-transform: rotate(0);
    transition: all ease 1s;
    -webkit-transition: all ease 1s;
    -moz-transition: all ease 1s;
    -o-transition: all ease 1s;
    margin-top: 3px;
    margin-left: 3px;
    width: 70px;
    height: 70px;
    padding: 1px;
    border-radius: 100%
}

.links ul li:hover img {
    transform: rotate(360deg);
    -webkit-transform: rotate(360deg);
    -moz-transform: rotate(360deg);
    -o-transform: rotate(360deg);
    -ms-transform: rotate(360deg)
}

.links ul li {
    border: 1px solid #dda6a6;
}
.links ul li img {
    box-shadow: inset 0 0 10px #dda6a6;
}

li.link-item {
    text-align: center;
}
.links ul li img{
	float:none;
}

.content {
